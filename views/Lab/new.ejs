            <label for="inputEmail" class="form-label">Email</label>
            <input type="text" name="email" class="form-control" id="inputName" placeholder="">
        </div>
        <div class="form-group ">
            <label for="inputName" class="form-label">Publication Link</label>
            <input type="text" name="publicationLink" class="form-control" id="inputName" placeholder="">
        </div>
