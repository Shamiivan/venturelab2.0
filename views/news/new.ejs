<h1>News New</h1>

<%include ../partials/header%>

<div class="container ">
    <form action="/projects/<%=project._id%>/news/" method="POST" enctype="multipart/form-data">
        <div class="form-group ">
            <label for="inputName" class="form-label">Title</label>
            <input type="text" name="title" class="form-control" id="inputName" placeholder="Title">
        </div>
        <div class="form-group">
            <label class="form-label" for="projectDescription">A little Description</label>
            <textarea name="description" class="form-control" id="projectDescription" rows="4"></textarea>
        </div>
        <div class="form-group ">
            <label for="inputName" class="form-label">Link</label>
            <input type="text" name="url" class="form-control" id="inputName" placeholder="Title">
        </div>
        <div class="form-group ">
            <label for="inputName" class="form-label">Type</label>
            <input type="text" name="type" class="form-control" id="inputName" placeholder="Title">
        </div>


        <div class="form-group">
            <label for="imageFile">Example file input</label>
            <input type="file" name="image" class="form-control-file" id="imageFile">
        </div>

        <button> SUBMIT</button>
    </form>
    <hr>

</div> <a href="/projects/header/new/">Add a new <section></section></a>