<% include ../partials/header %>

<div class="container">
    <div class="row">
        <%Projects.map((project)=>{%>
        <div class="col-md-4">
            <div style="background:url(<%= project.image%>); height:200px;"></div>
            <h3><%= project.name%></h3>
            <p><%= project.description%></p>
        </div>
        <a href="/projects/<%=project.id%>/edit">Update this project</a>
        <% })%>

    </div>

</div>

<div class="col-12">
    <a href="/projects/new" class="button cta">
        <span>Add A new Project</span>
        <svg width="13px" height="10px" viewBox="0 0 13 10">
            <path d="M1,5 L11,5"></path>
            <polyline points="8 1 12 5 8 9"></polyline>
        </svg>
    </a>
</div>


<%include ../partials/footer%>